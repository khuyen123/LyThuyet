            <ImageButton
                android:id="@+id/imageButton2D"
                android:layout_width="115dp"
                android:layout_height="115dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/sendo_hoantien"
                app:layout_constraintStart_toEndOf="@+id/imageButtonD"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:layout_width="115dp"
                android:layout_height="115dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/sendo_hoantien"
                app:layout_constraintStart_toEndOf="@+id/imageButton2D"
                app:layout_constraintTop_toTopOf="parent" />
        </android.support.constraint.ConstraintLayout>


        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="35dp"
            android:background="@drawable/button"
            android:padding="10dp"
            android:text="Làm mới"
            android:drawableLeft="@drawable/ic_baseline_refresh_24"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="20dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout10" />
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>